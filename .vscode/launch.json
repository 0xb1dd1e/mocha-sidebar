// A launch configuration that compiles the extension and then opens it inside a new window
{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch Extension",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "${workspaceRoot}/test/workspace"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Launch etcd",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "/home/matyz/dev/etcd.rf"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Launch nooow",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "C:\\dev\\noooow"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Launch gateway",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "/home/matyz/dev/gateway"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Launch producer-consumer",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "/home/matyz/dev/raf-producer-consumer/"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Launch Tests",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "--extensionTestsPath=${workspaceRoot}/test"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Attach child",
            "type": "node",
            "request": "attach",
            "port": 5859,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "outFiles": [],
            "localRoot": "${workspaceRoot}",
            "remoteRoot": null,
            "protocol": "inspector"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch recurse",
            "program": "${workspaceRoot}/stub/recursion"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch stub",
            "program": "${workspaceRoot}/inProcess/stub"
        },
    ]
}